{
  "name": "paliga",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:docs tw",
    "dev:docs": "vite --host --mode docs",
    "build": "tsc && vite build",
    "build:docs": "vite build --mode docs",
    "preview:docs": "vite preview --outDir dist-docs",
    "prepublishOnly": "npm run build",
    "tw": "npx tailwindcss -i ./src/apps/docs/input.css -o ./src/apps/docs/output.css --watch",
    "vitest": "vitest",
    "vitest:run": "vitest run",
    "pl": "npx playwright test",
    "pl:ui": "npx playwright test --ui"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@nextui-org/react": "^2.6.8",
    "@playwright/test": "^1.49.1",
    "@rollup/plugin-swc": "^0.4.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "ajv": "^8.17.1",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^2.3.3",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "framer-motion": "^11.15.0",
    "glob": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "react-router-dom": "^7.1.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-lib-inject-css": "^2.1.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  }
}
